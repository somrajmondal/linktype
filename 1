import bs4
import requests
url_list=["https://indianchacha.com/bird-sanctuaries-in-karnataka/"]
nofollow_word='nofollow'
noopenner_word='noopener'
total_nofollow_outbound_link=[]
total_nopener_word=[]

for url in url_list:
    res=requests.get(url)
    soup=bs4.BeautifulSoup(res.content,'lxml')
    find_noopenner_word=soup.find_all(noopenner_word)
    count_noopener_word=len(find_noopenner_word)
    for nw in find_noopenner_word:
         total_nopener_word.append(nw)
    print(total_nopener_word)



